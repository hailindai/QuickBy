<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.florist.announcement.mapper.AnnouncementMapper">
	<select id="showAll" resultType="AnnouncementInfo">
		select * from announcement_info;
	</select>
	
	<select id="showById" parameterType="string" resultType="AnnouncementInfo">
		select * from announcement_info where announcement_id=#{announcementId};

	</select>
</mapper> 












